<!DOCTYPE html>
<html>
  <head>
    <title>GaloreTestProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <%# This allows for the use of controller and action specific CSS and JS %>
  <body class="<%= params[:controller] %> <%= params[:action] %>">
    <%= yield %>
  </body>
</html>
